{
  "compilerOptions": {
    "jsx": "react",
    "module": "commonjs",
    "preserveConstEnums": true,
    "noImplicitAny": true,
    "noEmitOnError": true,
    "sourceMap": true,
    "target": "es5",
    "experimentalDecorators": true,
    "emitDecoratorMetadata": true,
    "lib": [ "es2015", "es5", "dom" ]
  },
  "files": [
    "./app.tsx",
    "./hello.tsx"
  ],
  "exclude": [
    "node_modules"
  ],
  "compileOnSave": true
}
